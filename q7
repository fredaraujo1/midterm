import random

random_numbers = []
for i in range(10):
    random_numbers.append(random.randint(1, 100))

# Remove numbers greater than 50 and replace others with "XX"
for i in range(len(random_numbers)):
    if random_numbers[i] > 50:
        random_numbers[i] = "XX"
    else:
        random_numbers[i] = "XX"

print(random_numbers)